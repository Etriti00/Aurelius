    "@typescript-eslint/eslint-plugin": "6.17.0",
    "@typescript-eslint/parser": "6.17.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.0",
    "jest": "29.7.0",
    "prettier": "3.1.1",
    "source-map-support": "0.5.21",
